	<!-- RCS Settings --><!-- VZW check and confirm -->
	<characteristic type="APPLICATION">
		<parm name="APPID"                    value="ap2002"/>	
		<characteristic type="IMS">
			<parm name="TO-APPREF"             value="ims-rcs"/>
		</characteristic>	
		<characteristic type="PRESENCE"> <!-- VZW check and confirm -->
			<parm name="usePresence"           value="1"/>
			<parm name="source-throttlepublish" value="30" />
			<parm name="max-number-ofsubscriptions-inpresence-list" value="100"/>
            <parm name="PublishTimer" value="1200"/>
			<characteristic type="Location">
				<parm name="TextMaxLength" value="0"/>
				<parm name="LocInfoMaxValidTime" value="0"/>
			</characteristic>
			<characteristic type="FAVLINK">
				<parm name="AutMa" value="Auto"/>
				<parm name="LabelMaxLength" value="0"/>
			</characteristic>
		</characteristic>
		<characteristic type="XDMS">
			<parm name="RevokeTimer" value="0"/>			
		</characteristic>
		<characteristic type="IM">
			<!--IM-->
			<parm name="imMsgTech"             value="1"/> <!-- Indicates, the Chat messaging Tech to be used [0: SIMPLE  1: CPM] -->
			<parm name="imCapAlwaysON"         value="0"/> <!-- Store&Forward, Set to 1- All RCS usrs are always avaliable else wrks based on alogorithm Sec 2.7.1.1 of RCS 5.1 V 2.0 spec -->
			<parm name="imWarnSF"              value="0"/> <!-- If imCapAlwaysON is set to 1 & imWarnSF is set to 1, gives waring to user whne initaite chat with offline usr -->
			<parm name="imSessionStart"        value="0"/> <!-- Indicates, the trigger point to send 200OK -->						
			<parm name="SmsFallBackAuth"       value="1"/> <!-- Indicates, SMS Fall back is enabled or not. -->
			<parm name="AutAccept"             value="1"/> <!-- Indicates, Session will be accepted automatically with out user interaction -->
			<parm name="AutAcceptGroupChat"    value="1"/> <!-- Indicates, Session will be accepted automatically with out user interaction -->
			<parm name="MaxSize1to1"           value="20480"/> <!-- VZW should decide this value. How much is the size(in Bytes) they wanted to send in a single chat message. If Chat message size exceeds this value, Application should promt an error to user -->
			<parm name="MaxSize1toM"           value="20480"/> <!-- VZW should decide this value. Same as MaxSize1to1 param, but this is for Group Chat session-->
			<parm name="TimerIdle"             value="660"/> <!-- IM SESSION TIMER in Table 77 of RCS 5.1. Recommended is 3 minutes As per Spec. But VZW Can provide their value, which ever they wanted to close the Inactive session -->
			<!--parm name="pres-srv-cap"       value="sip:foo@bar"/>--> <!-- SIMPLE Not used in RCS as per RCS 5.1 V 2.0 Spec -->
			<!--parm name="deferred-msg-func-uri" value="sip:foo@bar"/>--><!-- SIMPLE, Not used in RCS as per RCS 5.1 V 2.0 Spec-->
			<parm name="max_adhoc_group_size"  value="10"/> <!-- even though it is a SIMPLE param. It is used for CPM also in case of Group Chat, as per RCS 5.1 V 2.0 Spec, VZW should provide this LIMIT -->
			<!--<parm name="conf-fcty-uri"     value="sip:foo@bar"/> --> <!-- SIMPLE IM -->
			<!--<parm name="exploder-uri"      value="sip:foo@bar"/>--> <!-- SIMPLE, Not used in RCS as per RCS 5.1 V 2.0 Spec -->			
			<parm name="firstMessageInvite"    value="0"/> <!-- Indicates, whether 1st message to be sent in INVITE or not -->									
			<parm name="CPMControllingFuncUri" value="sip:confserver@vzims.com"/> <!-- VZW Provide, Value will be similar to that of conf-fcty-uri, if RCS is the service else different -->									
			<!--FT-->
			<parm name="ftWarnSize"            value="0"/> <!-- To Warn the user, if FT size exceeds this value. VZW should provide the size, when does they wanted to warnperforming FT. Should be in KB Say Ex: 8MB (i.e. 8192 KB)-->
			<parm name="MaxSizeFileTr"         value="10240"/> <!-- MAX allowed filesize to be transferred in KB, Ex: 10 MB (i.e. 10240 KB) -->
			<parm name="ftAutAccept"           value="0"/> <!-- Indicates, All FT Session will be accepted automatically with out user interaction -->
			<parm name="ProvideFT"             value="1"/> <!-- Indicates, whether FT service is enabled or disabled. For VZW it is 1, As VZW provides FT service -->
			<parm name="ftDefaultMech"         value="MSRP"/>
		</characteristic>
		<characteristic type="cpm">
			<characteristic type="standalonemsg">
				<parm name="MaxSizeStandalone"         value="1048576"/> <!-- Refer Table77 in RCS 5.1 V 2.0 Spec. For VZW, It will be 1MB -->
			</characteristic>
		</characteristic>
		<characteristic type="services">
			<parm name="ChatAuth"              value="1"/> <!-- Indicates, weather CAHT service is enabled or not -->
			<parm name="SLMAuth"               value="1"/> <!-- Indicates, whether SLM service is enabled or disabled. For VZW it is 1, As VZW provides SLM service -->
		</characteristic>
		<characteristic type="CAPDISCOVERY"> <!-- These values are need for client. So VZW can set them to 0 -->
                        <parm name="pollingPeriod"         value="604800"/>
                        <parm name="capinfoexpiry"         value="60"/>
			<!-- <parm name="presenceDisc"          value="0"/> -->
			<parm name="defaultDisc"              value="1"/>
			<parm name="capDiscCommonStack"       value="0"/>
		</characteristic>
		<characteristic type="APN">
			<parm name="rcseOnlyAPN"           value="ims"/>
			<parm name="alwaysuseimsapn"       value="1"/>
			<parm name="enableRcseSwitch"      value="1"/>
		</characteristic>
		<characteristic type="OTHER">
			<parm name="endUserConfReqId" value="31148012244004245@vzims.com"/> <!-- VZW provide -->
			<characteristic type="transportProto">
				<parm name="psSignalling"   value="SIPoUDP"/>
				<parm name="psMedia"        value="MSRP"/>
				<parm name="psRTMedia"      value="RTP"/>
			</characteristic>
		</characteristic>
		<characteristic type="SERVICES">
		  <parm name="presencePrfl"       value="0"/>
		  <parm name="ChatAuth"           value="1"/>
		  <parm name="GroupChatAuth"      value="1"/>
		  <parm name="ftAuth"             value="1"/>
		  <parm name="standaloneMsgAuth"  value="1"/>
		  <parm name="geolocPullAuth"     value="0"/>
		  <parm name="geolocPushAuth"     value="0"/>
		  <parm name="vsAuth"             value="0"/>
		  <parm name="isAuth"             value="0"/>
		  <parm name="rcsIPVoiceCallAuth" value="0"/>
		  <parm name="rcsIPVideoCallAuth" value="0"/>
		</characteristic>
	</characteristic>
