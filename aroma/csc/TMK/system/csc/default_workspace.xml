<?xml version="1.0" encoding="utf-8" ?>
<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <home>
        <!--  screen [1]  -->
        <!-- Weather widget  -->
        <appwidget
            launcher:packageName="com.sec.android.daemonapp"
            launcher:className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1"
            launcher:screen="0"
            launcher:x="0"
            launcher:y="0"
            launcher:spanX="2"
            launcher:spanY="1" />

        <!-- .SearchWidgetProvider -->
        <appwidget
            launcher:packageName="com.google.android.googlequicksearchbox"
            launcher:className="com.google.android.googlequicksearchbox.SearchWidgetProvider" 
            launcher:screen="0" 
            launcher:x="0" 
            launcher:y="2"
            launcher:spanX="4"
            launcher:spanY="1" />

        <!-- Email -->
        <favorite
            launcher:packageName="com.samsung.android.email.provider" 
            launcher:className="com.samsung.android.email.ui.activity.MessageListXL" 
            launcher:screen="0"
            launcher:x="0"
            launcher:y="3" />

        <!-- Camera -->
        <favorite
            launcher:packageName="com.sec.android.app.camera"
            launcher:className="com.sec.android.app.camera.Camera"
            launcher:screen="0"
            launcher:x="1"
            launcher:y="3" />

        <!-- Play Store -->
        <favorite
            launcher:packageName="com.android.vending"
            launcher:className="com.android.vending.AssetBrowserActivity"
            launcher:screen="0"
            launcher:x="2"
            launcher:y="3" />

        <!-- Folder : Google -->
        <folder
            launcher:screen="0"
            launcher:title="Google"
            launcher:x="3"
            launcher:y="3" >
            <!-- Google -->
            <favorite
                launcher:screen="0"
                launcher:packageName="com.google.android.googlequicksearchbox"
                launcher:className="com.google.android.googlequicksearchbox.SearchActivity" />
             <!-- Chrome -->
            <favorite
                launcher:screen="1"
                launcher:packageName="com.android.chrome"
                launcher:className="com.google.android.apps.chrome.Main" />				
            <!-- Gmail -->
            <favorite
                launcher:screen="2"
                launcher:packageName="com.google.android.gm"
                launcher:className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Maps -->
            <favorite
                launcher:screen="3"
                launcher:packageName="com.google.android.apps.maps"
                launcher:className="com.google.android.maps.MapsActivity" />
            <!-- YouTube -->
            <favorite
                launcher:screen="4"
                launcher:packageName="com.google.android.youtube"
                launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />
            <!-- Drive -->
            <favorite
                launcher:screen="5"
                launcher:packageName="com.google.android.apps.docs"
                launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- Play Music -->
            <favorite
                launcher:screen="6"
                launcher:packageName="com.google.android.music"
                launcher:className="com.android.music.activitymanagement.TopLevelActivity" />
            <!-- Play Movies & TV -->
            <favorite
                launcher:screen="7"
                launcher:packageName="com.google.android.videos"
                launcher:className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Hangouts -->
            <favorite
                launcher:screen="8"
                launcher:packageName="com.google.android.talk"
                launcher:className="com.google.android.talk.SigningInActivity" />
            <!--Photos-->
            <favorite 
                launcher:screen="9"
                launcher:packageName="com.google.android.apps.photos"
                launcher:className="com.google.android.apps.photos.home.HomeActivity" />
        </folder> 


        <!--  screen [1]  -->
        <!-- .mp5.lib.newsreader.ui.MobitilesWidgetProvider -->
        <appwidget
            launcher:packageName="com.mobileposse.client" 
            launcher:className="com.mobileposse.client.mp5.lib.newsreader.ui.MobitilesWidgetProvider"
            launcher:screen="1"
            launcher:x="0"
            launcher:y="0"
            launcher:spanX="4"
            launcher:spanY="2" />

        <!-- Gallery -->
        <favorite
            launcher:packageName="com.sec.android.gallery3d"
            launcher:className="com.sec.android.gallery3d.app.GalleryOpaqueActivity"
            launcher:screen="1"
            launcher:x="0"
            launcher:y="3" />


        <!-- folder -->
        <folder
            launcher:screen="1"
            launcher:title="MetroPCS"
            launcher:x="3"
            launcher:y="3" >

            <!-- Metro App store -->
            <favorite
                launcher:screen="0"
                launcher:packageName="com.handmark.metro.launcher"
                launcher:className="com.handmark.metro.launcher.Launcher" />

            <!-- Block it -->
            <favorite
                launcher:screen="1"
                launcher:packageName="com.privacystar.android.metro"
                launcher:className="com.privacystar.core.ui.EulaActivity" />

            <!-- Lookout -->
            <favorite
                launcher:screen="2"
                launcher:packageName="com.lookout"
                launcher:className="com.lookout.ui.LoadDispatch" />

            <!-- metroZONE -->
            <favorite
                launcher:screen="3"
                launcher:packageName="com.mobileposse.client"
                launcher:className="com.mobileposse.client.mp5.lib.view.screen.ControllerActivity" />

            <!-- MyMetro -->
            <favorite
                launcher:screen="4"
                launcher:packageName="com.nuance.nmc.sihome.metropcs"
                launcher:className="com.nuance.nmc.sihome.metropcs.myMetro" />

            <!-- .Mobile hotspot -->
            <favorite
                launcher:screen="5"
                launcher:packageName="com.android.settings"
                launcher:className="com.android.settings.Settings$WifiApSettingsActivity" />

            <!-- Visual voice mail -->
            <favorite
                launcher:screen="6"
                launcher:packageName="com.metropcs.service.vvm"
                launcher:className="com.metropcs.service.vvm.activity.setup.WelcomeActivity" />

            <!-- Remote SIM Unlock -->
            <favorite
                launcher:screen="7"
                launcher:packageName="com.metro.simlock"
                launcher:className="com.metro.simlock.WelcomeActivity" />
        </folder>

        <!--  screen [2]  -->
        <!-- .WidgetProvider42 -->
        <appwidget
            launcher:packageName="com.sec.android.widgetapp.samsungapps"
            launcher:className="com.sec.android.widgetapp.samsungapps.WidgetProvider42"
            launcher:screen="2"
            launcher:x="0"
            launcher:y="0"
            launcher:spanX="4"
            launcher:spanY="1" />

       <!-- Samsung Pay -->
       <favorite
            launcher:packageName="com.samsung.android.spay"
            launcher:className="com.samsung.android.spay.ui.SpayMainActivity"
            launcher:screen="2"
            launcher:x="0"
            launcher:y="3" />


        <!-- Calendar -->
        <favorite
            launcher:packageName="com.android.calendar"
            launcher:className="com.android.calendar.AllInOneActivity"
            launcher:screen="2"
            launcher:x="1"
            launcher:y="3" />

        <!-- Facebook -->
        <favorite
            launcher:packageName="com.facebook.katana"
            launcher:className="com.facebook.katana.LoginActivity"
            launcher:screen="2"
            launcher:x="2"
            launcher:y="3" />

        <!-- S Health -->
        <favorite
            launcher:packageName="com.sec.android.app.shealth"
            launcher:className="com.samsung.android.app.shealth.home.HomeMainActivity" 
            launcher:screen="2"
            launcher:x="3"
            launcher:y="3" />

    </home> 

    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <hotseat>
        <!-- Phone -->
        <favorite
            launcher:packageName="com.android.contacts"
            launcher:className="com.android.dialer.DialtactsActivity"
            launcher:x="0" />

        <!-- Messages -->
        <favorite
            launcher:packageName="com.android.mms"
            launcher:className="com.android.mms.ui.ConversationComposer"
            launcher:x="1" />

        <!-- Internet -->
        <favorite
            launcher:packageName="com.sec.android.app.sbrowser"
            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity"
            launcher:x="2" />
    </hotseat> 

</favorites> 
